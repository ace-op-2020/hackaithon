# Google Cloud Configuration
# Copy this file to .env and set your Google Cloud project details

# Google Cloud Project ID
REACT_APP_GOOGLE_CLOUD_PROJECT_ID=svc-hackathon-prod15

# Google Cloud Region (for Vertex AI)
REACT_APP_GOOGLE_CLOUD_REGION=us-central1

# Gemini Model to use
REACT_APP_GEMINI_MODEL=gemini-1.5-flash

# Authentication Method 1: Google Cloud Access Token (preferred for production)
# REACT_APP_GOOGLE_CLOUD_ACCESS_TOKEN=your_access_token_here

# Authentication Method 2: Google AI API Key (easier for development)
# Get from: https://makersuite.google.com/app/apikey
REACT_APP_GOOGLE_API_KEY=your_api_key_here

# Kapture Server (default)
REACT_APP_KAPTURE_SERVER_URL=http://localhost:61822

# Alternative: If you want to use OpenAI instead
# REACT_APP_LLM_ENDPOINT=https://api.openai.com/v1/chat/completions
# REACT_APP_LLM_API_KEY=your_openai_api_key_here 